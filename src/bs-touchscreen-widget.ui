<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="BsTouchscreenWidget" parent="GtkWidget">

    <child>
      <object class="GtkFlowBox" id="slots_flowbox">
        <property name="selection-mode">single</property>
        <property name="homogeneous">True</property>
        <property name="min-children-per-line">1</property>
        <property name="max-children-per-line">4</property>
        <signal name="selected-children-changed" handler="on_slots_flowbox_selected_children_changed_cb" object="BsTouchscreenWidget" swapped="no" />
      </object>
    </child>

  </template>
</interface>
